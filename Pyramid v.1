public class Pyramid
{
    public static void main(String[] args)
    {
        String val = "0";
        String me = " ";

        System.out.println(me.repeat(5) + val.repeat(1));
        System.out.println(me.repeat(4) + val.repeat(3));
        System.out.println(me.repeat(3) + val.repeat(5));
        System.out.println(me.repeat(2) + val.repeat(7));
        System.out.println(me.repeat(1) + val.repeat(9));
        System.out.println(val.repeat(11));
    }
}
